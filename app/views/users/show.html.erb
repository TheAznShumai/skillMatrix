<h1>Users#show</h1>

<table>
    <tr>
        <th>User ID</th>
        <th>Skill Name</th>
        <th>Score</th>
    </tr>
    <% @user.skills.each do |skill| %>
        <tr>
            <td><%= @user.email %></td>
            <td><%= skill.name %></td>
            <td><%= skill.rating.score %></td>    
        </tr>
    <% end %>
</table>

