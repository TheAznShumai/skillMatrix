<h1>Survey</h1>
<ul id="filterOptions">
  <li class="active"><a href="#" class="all">All</a></li>
  <li><a href="#" class="prem">Premier League</a></li>
  <li><a href="#" class="champ">Championship</a></li>
  <li><a href="#" class="league1">League 1</a></li>
  <li><a href="#" class="league2">League 2</a></li>
</ul>

<ul class="holder list-inline">
  <% @surveys.each_with_index do |survey, index| %>
        <li class="item col-md-2 text-center" data-id=<%= "#{survey.id}" %> data-type="league1">
        <%= link_to image_tag(survey.icon(:small)), survey%><br/>
        <%= link_to survey.name,
            new_or_edit_attempt_path(survey, current_user.attempts.where(:survey_id => survey.id)) %>
    </li>
  <% end %>
</ul>

