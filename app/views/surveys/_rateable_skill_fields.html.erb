
<fieldset>
  <div class="panel panel-primary">
    <div class="panel-heading">
      Please rate yourself on the following.
    </div>
    <div class="panel-body inline">
      <%= f.text_field :name, :class => "form-control", :placeholder => "Enter a SkillName" %>
      <ul class="list-inline pull-right">
        <% (1..5).each do |i| %>
          <li class="rating_star" id="stub_form_<%= i %>" data-stars="<%= i %>" data-form-id="stub_form"></li>
        <% end %>
      </ul>
    </div>
    <div class="panel-footer clearfix">
      <div class="pull-right">
        <%= f.hidden_field :_destroy %>
        <%= link_to "remove", '#', :class => "btn btn-danger remove_fields" %>
      </div>
    </div>
  </div>
</fieldset>

